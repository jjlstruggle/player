{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@apis/*": [
        "./src/apis/*"
      ],
      "@utils": [
        "./src/utils"
      ],
      "@components": [
        "./src/components/*"
      ],
      "@assets": [
        "./src/assets"
      ],
      "@hooks": [
        "./src/hooks"
      ],
      "@styles": [
        "./src/styles"
      ],
      "@pages": [
        "./src/pages"
      ],
      "@store": [
        "./src/store"
      ],
    }
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "ts-node": {
    "esm": true
  }
}